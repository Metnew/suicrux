{
    "plugins": ["lodash",
        [
            "transform-runtime", {
                "helpers": false,
                "polyfill": false,
                "regenerator": true,
                "moduleName": "babel-runtime"
            },
        ],
        "react-hot-loader/babel"
    ],
    "presets": ["es2015", "react", "stage-0"],
    "env": {
        "ssr": {
            "plugins": [
                ["babel-plugin-webpack-alias", {
                    "config": "webpack_config/webpack.base.js"
                }]
            ]
        },
        "production": {
            "presets": ["react-optimize"]
        }
    }
}
